    layer.depth = depth
    layerObj.parent = id
    }
  uiSetClickable(alist,false)
  }
else if PBack.clicked {
  with parent.bNewGame {uiOnclick()}
  uiSetClickable(alist,false)
  }
else if bYes != -1 
  if bYes.clicked {
    global.GameType = 3
    instance_destroy(parent, true)
    room_goto(rGame)

        <arguments>
          <argument>
            <kind>1</kind>
            <string>//cheat processor, performed from MenuRuler.b2Accept
if cheat == 'STOP'
  chStop = true
else if (cheat == 'REVERSE')||(cheat == 'TURN')
  if chReverse
    chReverse = false
  else
    chReverse = true
else if cheat == 'START'
  chStop = false
else if ((cheat == 'EASTER') || (cheat == 'EASTEREGG'))
  with parent {bEggClicked = true; event_perform(ev_mouse,ev_global_left_release)}
else if (cheat == 'PREVED') || (cheat == 'HELLO')
  instance_create(0,0,oPreved)
else if cheat == 'SPLASH'
  with parent {http_get("http://sooslick.flaminggaming.ru/wl/getSplash.php")}
else if (cheat == 'EXIT')||(cheat == 'QUIT')
  {
  with parent {
    bExit.clicked = true
    event_perform(ev_mouse, ev_global_left_release)
    }
  }
else if cheat == 'BLUR'
  chBlur = true
else if (cheat == 'FLIP') || (cheat == 'SPIN')
  {
  if chFlip
    chFlip = false
  else
    chFlip = true
  if isReverse
    isReverse = false
  else
    isReverse = true
  }
else if cheat == 'SHUFFLE'
  for (i=0; i&lt;ARRMAX; i++)
    for (j=0; j&lt;ARRMAX; j++)
      {
      if random(1) &gt; 0.69
        continue
      var i1 = irandom_range(0,ARRMAX-1);
      var j1 = irandom_range(0,ARRMAX-1);
      var tmp = sqArr[i,j].content;
      sqArr[i,j].content = sqArr[i1,j1].content
      sqArr[i1,j1].content = tmp
      }
else if cheat == 'RESTART'
  {
  chRestart = true
  event_user(1)
  }
else if cheat == 'RESET'
  event_user(1)
else if cheat == 'CANCEL'
  {
  chStop = false
  chReverse = false
  chBlur = false
  if chFlip
    {
    chFlip = false
    if isReverse
      isReverse = false
    else
      isReverse = true
    }
  }
      
//refresh
cheat = ''
for (i=0; i&lt;ARRMAX; i++)
  for (j=0; j&lt;ARRMAX; j++)
    {
    sqArr[i,j].clr = c_white    //change color
    sqArr[i,j].spr = 0
    }
</string>

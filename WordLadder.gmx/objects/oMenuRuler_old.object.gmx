        <arguments>
          <argument>
            <kind>1</kind>
            <string>if async_load[? "status"] != 0
  exit

var html = json_decode(async_load[? "result"])
if html != -1
  {
  if !is_undefined(html[?'CONTENT'])
    {
    motd = html[?'CONTENT']
    motd_ready = true
    exit
    }
  if !is_undefined(html[?'VERSION'])
    {
    var v = real(html[?'VERSION'])
    if global.GameVersion &gt; v
      checkVersionStr = 'Server outdated'
    else if global.GameVersion &lt; v
      checkVersionStr = 'GAME OUTDATED'
    //check extra dict ver
    //TODO LOCALE CHECK
    var e = real(html[?'EXTRA']);
    if e != global.config[?'ExtraDict']
      {
      //TODO: update ver after downloading;
      //TODO: downloader obj + progbar
      //TODO: alert!
      http_get_file("http://" + html[?'EXTRA_PATH'], "dict/EN_extra.dat")
      global.config[?'ExtraDict'] = e
      }
    exit
    }
  }
</string>
          </argument>
        </arguments>
      </action>
    </event>

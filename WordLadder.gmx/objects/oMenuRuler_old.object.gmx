b2Accept.content = 'ACCEPT'
b2Accept.dsc = 0.66
b2Accept.tsc = 0.65
//Background processor
bg = instance_create(0,0,oSquareRuler)
bg.parent = id
bg.b21 = b2Hide     //pointers to overlapping buttons
bg.b22 = b2Accept
motd = ""

var dirs_arr;
while dir != "" {
    dirs_arr[dirs_total] = 'cache/'+dir
    dirs_total++
    dir = file_find_next()
    }
dirs_total--
while (dirs_total &gt;= 0) {
    //read content from cache
    Content[Headlines] = ''
    var f = file_text_open_read(dirs_arr[dirs_total]+'/content')
    while (!file_text_eof(f)) {
        Content[Headlines]+= file_text_read_string(f) + '#';
        file_text_readln(f);
        }
    file_text_close(f);
    Content[Headlines] = formatHtmlString(Content[Headlines])
    //read img
    if file_exists(dirs_arr[dirs_total]+'/img')
        Img[Headlines] = sprite_add(dirs_arr[dirs_total]+'/img',0,false,false,0,0)
    else
        Img[Headlines] = -1
    //set pos on screen
    ContentPos[Headlines] = MaxPos
    draw_set_font(fChat)
    MaxPos+= string_height_ext(Content[Headlines],50,1450)
    ImgPos[Headlines] = MaxPos
    if Img[Headlines] != -1
        MaxPos+= sprite_get_height(Img[Headlines]) 
    LinePos[Headlines] = MaxPos + 12
    MaxPos+= 24
    //next
    Headlines++
    dirs_total--
    }

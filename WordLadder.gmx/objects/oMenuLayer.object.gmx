            <kind>1</kind>
            <string>if Anim
  {
  //animation tick
  AnimTick+= 1
  var p = funcTickInterpolate(AnimTick, AnimTickMax);
  if AnimTick &gt;= AnimTickMax {
    AnimTick = 0
    Anim = false
    }
  switch AnimType {
    case 0:                 //anim rot in
      var ag = 90*p + 90;
      var cRb = cR * (1.5-p/2);
      sfx = cRb*dcos(ag) + cx
      sfy = -cRb*dsin(ag) + cy
      sfr = -45 + 45*p
      sfa = p
      sfsc = 2-p
      break;
    case 1:               //anim rot out
      var ag = 90*p+180;
      var cRb = cR * (1+p/2);
      sfx = cRb*dcos(ag) + cx
      sfy = -cRb*dsin(ag) + cy
      sfr = 45*p
      sfa = 1-p
      sfsc = 1+p
      if AnimTick == 0
        instance_destroy(self, true)
      break;
    case 2:             //anim shift in
      sfx = cx - (global.G_RIGHT*p)
      sfy = cy
      sfr = 0
      sfa = p
      break
    case 3:             //anim shift out
      sfx = 50 - (global.G_RIGHT*p)
      sfy = cy
      sfr = 0
      sfa = p
      if AnimTick == 0
        instance_destroy(self, true)
    }
  }
</string>

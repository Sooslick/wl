    layer.AnimType = 1
    layer.depth++
    layerObj = instance_create(0,0,oProfileStats) //layer: create new
    layer = instance_create(0,0,oMenuLayer)
    layer.bg = bg
    layer.Child = layerObj
    layer.depth = depth
    layerObj.parent = id
    }
  uiSetClickable(alist,false)
  }

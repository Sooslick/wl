  if bmf.checked
    stats.Selected = 7
  else
    stats.Selected = 6
  }
else if bm40.clicked
  {
  bm40.clicked = false
  if bmf.checked
    stats.Selected = 9
  else
    stats.Selected = 8
  }
else if bmg.clicked
  {
  bmg.clicked = false
  stats.Selected = 1
  }
//todo others
else if bReset.clicked
  {
  bReset.clicked = false
  bReset.clickable = false
  bBack.clickable = false
  bAlert = instance_create(0,0,oConfirmButton)
  bAlert.content = 'Reset all stats?'
  }
else if bAlert&lt;&gt;0
  {
  if bAlert.bYes.clicked
    {
    global.profile[?'PSINGAMETIME'] = 0
    global.profile[?'PSPLAYTIME'] = 0
    global.profile[?'PSONLINETIME'] = 0
    global.profile[?'PSGAMES'] = 0
    global.profile[?'PSENDS'] = 0
    global.profile[?'PSSERVERS'] = 0
    global.profile[?'PSRESETS']++
    ds_map_destroy(global.profile[?'PSSECTIONS'])
    global.profile[?'PSSECTIONS'] = ds_map_create()
    instance_destroy(bAlert,true)
    bAlert = 0
    bReset.clickable = true
    bBack.clickable = true
    }
  else if bAlert.bNo.clicked
    {
    instance_destroy(bAlert,true)
    bAlert = 0
    bReset.clickable = true
    bBack.clickable = true
    }
  }
else if bBack.clicked
  {
  p = instance_create(0,0,oProfileRuler)
  p.parent = parent
  parent.ActiveMenuPtr = p
  parent.Header = 'PROFILE SETTINGS'
  instance_destroy(self,true)
  }

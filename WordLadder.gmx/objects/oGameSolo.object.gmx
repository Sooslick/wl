    //create confirmation window
    bConf = instance_create(0,0,oConfirmButton);
    bConf.parent = id
    bConf.content = 'Load game?#If no data will be rewritten'
    with bConf event_user(0) //set parent for buttons
    bYes = bConf.bYes
